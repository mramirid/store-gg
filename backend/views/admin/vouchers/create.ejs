<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/common-head-content.ejs') %>

  <!-- Select2 -->
  <link rel="stylesheet" href="/admin/admin-lte/plugins/select2/css/select2.min.css">
  <link rel="stylesheet" href="/admin/admin-lte/plugins/select2-bootstrap4-theme/select2-bootstrap4.min.css">
</head>

<%- include('../partials/dashboard/layout-start.ejs', {
  previousBreadcrumbs: [
    { label: "Admin", href: "/admin" },
    { label: "Vouchers", href: "/admin/vouchers" },
  ]
}) %>

<!-- Main content -->
<section class="content">
  <div class="container-fluid">

    <%- include('../partials/alert.ejs') %>

    <div class="row">
      <div class="col-12">
        <div class="card card-primary">
          <div class="card-header">
            <h3 class="card-title">New Voucher Game</h3>
          </div>
          <!-- /.card-header -->
          <!-- form start -->
          <form action="/admin/vouchers?_csrf=<%= csrfToken %>" method="POST" enctype="multipart/form-data">
            <div class="card-body">
              <%- include("./inputs.ejs") %>
            </div>
            <!-- /.card-body -->

            <div class="card-footer">
              <button type="submit" class="btn btn-primary">Save</button>
            </div>
          </form>
        </div>
        <!-- /.card -->
      </div>
      <!-- /.col -->
    </div>
    <!-- /.row -->
  </div>
  <!-- /.container-fluid -->
</section>
<!-- /.content -->

<%- include('../partials/dashboard/layout-end.ejs') %>

<%- include('../partials/common-scripts.ejs') %>

<!-- Select2 -->
<script src="/admin/admin-lte/plugins/select2/js/select2.full.min.js"></script>
<script nonce="<%= cspNonce %>">
  $(() => {
    //Initialize Select2 Elements
    $('.select2bs4').select2({ theme: 'bootstrap4' })
  });
</script>

<!-- bs-custom-file-input -->
<script src="/admin/admin-lte/plugins/bs-custom-file-input/bs-custom-file-input.min.js"></script>
<script nonce="<%= cspNonce %>">
  $(() => {
    bsCustomFileInput.init();
  });
</script>

</html>